{
  "manifest_version": 3,
  "name": "Professional Profile Analytics",
  "version": "1.3",
  "description": "This plugin connects to Professional Profile Analytics and automatically uploads your LinkedIn Analytics data.",
  "permissions": ["storage", "scripting", "downloads", "webRequest", "alarms"],
  "host_permissions": ["https://www.linkedin.com/*", "https://linkedin.com/*", "https://office.turtletrafo.com/*", "https://o4bc35iy43fytzunzwo3yiv2bi0agykt.lambda-url.eu-west-1.on.aws/", "https://5tkpkldrszif7wej5l6kdqam3u0upiid.lambda-url.eu-west-1.on.aws/", "https://cdn.jsdelivr.net/"],
  "content_security_policy": {
    "style-src": [
      "'self'",
      "https://cdn.jsdelivr.net"
    ]},
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon.png",
    "48": "icon48.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon48.png"
    }
  }
}
