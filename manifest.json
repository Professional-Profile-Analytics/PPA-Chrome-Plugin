{
  "manifest_version": 3,
  "name": "Professional Profile Analytics",
  "version": "1.3.1",
  "description": "This plugin connects to Professional Profile Analytics and automatically uploads your LinkedIn Analytics data.",
  "permissions": ["storage", "scripting", "downloads", "webRequest", "alarms"],
  "host_permissions": ["https://www.linkedin.com/*", "https://linkedin.com/*", "https://office.turtletrafo.com/*", "https://bzgaehjpqf45zsi4t32zuf6g7m0nrctb.lambda-url.us-east-1.on.aws/", "https://cwf6tbhekvwzbb35oe3psa7lza0oiaoj.lambda-url.us-east-1.on.aws/", "https://cdn.jsdelivr.net/"],
  "content_security_policy": {
    "style-src": [
      "'self'",
      "https://cdn.jsdelivr.net"
    ]},
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon.png",
    "48": "icon48.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon48.png"
    }
  }
}
